{
    "name": "bagsiz/easy-functions",
    "description": "Laravel package for useful & easy PHP functions",
    "keywords": [
        "bagsiz",
        "easy",
        "functions",
        "laravel"
    ],
    "homepage": "https://github.com/bagsiz/easy-functions",
    "authors": [
        {
            "name": "Murat Bağsız",
            "email": "murat@bagsiz.com",
            "homepage": "http://bagsiz.com"
        }
    ],
    "require": {
      "php" : "^7.2",
      "laravel/framework": "^5.4.0"
    },
    "autoload": {
      "psr-4": {
        "Bagsiz\\EasyFunctions\\": "src"
      }
    },
    "extra":{
      "laravel":{
        "providers":[
          "Bagsiz\\EasyFunctions\\EasyFunctionsServiceProvider"
        ],
        "aliases":{
          "EasyFunctions": "Bagsiz\\EasyFunctions\\EasyFunctionsFacade"
        }
      }
    },
    "autoload-dev": {
      "psr-4": {
        "Bagsiz\\EasyFunctions\\Tests\\": "tests"
      }
    }
}
